package com.DB.jpa.Repo;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;

import com.DB.jpa.Entity.Applicants;

public interface ApplicantRepo extends JpaRepository<Applicants,Integer> {

	List<Applicants> findByName(String name);


	List<Applicants> findByAgeGreaterThan(Integer age);


}
